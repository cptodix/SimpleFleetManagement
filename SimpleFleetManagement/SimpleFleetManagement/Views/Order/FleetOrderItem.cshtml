@model SimpleFleetManagement.ViewModel.FleetOrderViewModel

<tr>
    <td>
        @Html.EditorFor(model => model.FleetId, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } }) 
        <br />
        @Html.EditorFor(model => model.FleetName, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
    </td>

    <td>
        @Html.EditorFor(model => model.DriverId, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
        <br />
        @Html.EditorFor(model => model.DriverName, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
    </td>

    <td>
        @Html.EditorFor(model => model.AssitantId, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
        <br />
        @Html.EditorFor(model => model.AssitantName, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
    </td>
       
    <td>
        <button type="button" class="btn btn-danger btn-remove" onclick="RemoveItem(this)">Remove</button>
    </td>
</tr>

